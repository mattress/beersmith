<F_S_NAME>Christmas/Winter Specialty Spice Beer</F_S_NAME>
<F_S_CATEGORY>Spice/Herb/Vegetable Beer</F_S_CATEGORY>
<F_S_GUIDE>BJCP 2008</F_S_GUIDE>
<F_S_LETTER>2</F_S_LETTER>
<F_S_NUMBER>21</F_S_NUMBER>
<F_S_TYPE>2</F_S_TYPE>
<F_S_MIN_OG>1.0300000</F_S_MIN_OG>
